#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules/@dcloudio/vite-plugin-uni/bin/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules/@dcloudio/vite-plugin-uni/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules/@dcloudio/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules/@dcloudio/vite-plugin-uni/bin/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules/@dcloudio/vite-plugin-uni/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules/@dcloudio/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-4030620241128001_postcss@8.5.6_rollup@4.46.2_vite@5.4.1_e5a1bb936b835ad671c5cfa2f366fc13/node_modules:/persistent/home/Asoka/Code/nao-todo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
else
  exec node  "$basedir/../@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
fi
